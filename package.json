{
  "name": "pilum",
  "description": "Extremely Minimal Multicall3 Typescript Library",
  "author": "https://github.com/abigger87",
  "license": "AGPL-3.0-only",
  "version": "1.10.0",
  "repository": "git://github.com/abigger87/pilum.git",
  "publishConfig": {
    "@abigger87:registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@ethersproject/providers": "^5.6.6",
    "ethers": "^5.6.4"
  },
  "scripts": {
    "prepublish": "tsc && pkgfiles",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "tsc": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "size": "tarball=\"$(npm pack .)\"; wc -c \"${tarball}\"; tar tvf \"${tarball}\"; rm \"${tarball}\";"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "pkgfiles": "^2.3.2",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
